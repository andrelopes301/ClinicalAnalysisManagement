@model LabTestesOnline.Models.Teste

@{
    ViewData["Title"] = "Detalhes";
}

<br>
<h1>Detalhes do Teste</h1>
<hr style="border: 1px solid black;">
<br>

<div>
    <dl class="row">
        <dt class = "col-sm-2">
            Tipo de Teste
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TipoTeste)
        </dd>
        <dt class = "col-sm-2">
            Data Início
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataInicio)
        </dd>
        <dt class = "col-sm-2">
          Data Final
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataFinal)
        </dd>
        <dt class = "col-sm-2">
           Resultado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Resultado)
        </dd>
        <dt class = "col-sm-2">
            Centro de Análise
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CentroAnalise.Nome)
        </dd>
        <dt class = "col-sm-2">
            Técnico Responsável
        </dt>
         <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TecnicoResponsavel.Nome)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cliente)
        </dt>
          <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cliente.Nome)
        </dd>

    </dl>
</div>

<div>
    <a class="btn btn-dark"  asp-controller="Home" asp-action="Index">Voltar Atrás</a>
</div>
