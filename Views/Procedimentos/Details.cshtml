@model LabTestesOnline.Models.Procedimento

@{
    ViewData["Title"] = "Detalhes";
}

<h1>Detalhes do Procedimento</h1>

<div>
    <h4>Procedimento</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Procedimento
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TipoProcedimento)
        </dd>
        <dt class = "col-sm-2">
           Tipo de Teste
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TestesPossiveis.TipoTeste)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
    <a asp-action="Index">Voltar Atrás</a>
</div>
