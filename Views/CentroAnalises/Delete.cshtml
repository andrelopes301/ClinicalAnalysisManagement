@model LabTestesOnline.Models.CentroAnalise

@{
    ViewData["Title"] = "Centro de Análise";
}

<br>
<h1>Remover Centro de Análise</h1>
<hr style="border: 1px solid black;">
<br>

<div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Localidade)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Localidade)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NumLimiteTestes)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NumLimiteTestes)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.HorarioAbertura)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.HorarioAbertura)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.HorarioEncerramento)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.HorarioEncerramento)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Gestor)
        </dt>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Remover" class="btn btn-danger" /> |
        <a asp-action="Index">Voltar Atrás</a>
    </form>
</div>
